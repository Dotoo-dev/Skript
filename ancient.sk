command /tomottieffectdayoooo <player>:
	# executable by: console
	trigger:
		set {_animation.height.%player-arg's uuid%} to 0.0
		#set {_animation.ycoord.%player-arg's uuid%} to y coord of event-location
		loop 10 times:
			execute console command "mogsound %player-arg% aafutbeam 0.5 1.3"
			execute console command "/execute %player-arg% ~ ~ ~ /particle flame %x coord of event-location% %y coord of event-location + {_animation.height.%player-arg's uuid%}% %z coord of event-location% 0 0 0 0 30"
			add 0.1 to {_animation.height.%player-arg's uuid%}
			wait 0.1 seconds
		loop 10 times:
			execute console command "mogsound %player-arg% aafutbeam 0.5 1.6"
			execute console command "/execute %player-arg% ~ ~ ~ /particle flame %x coord of event-location% %y coord of event-location + {_animation.height.%player-arg's uuid%}% %z coord of event-location% 0 0 0 0 30"
			add 0.1 to {_animation.height.%player-arg's uuid%}
			wait 0.1 seconds
		loop 10 times:
			execute console command "mogsound %player-arg% aafutbeam 0.5 1.84"
			execute console command "/execute %player-arg% ~ ~ ~ /particle flame %x coord of event-location% %y coord of event-location + {_animation.height.%player-arg's uuid%}% %z coord of event-location% 0 0 0 0 30"
			add 0.1 to {_animation.height.%player-arg's uuid%}
			wait 0.1 seconds
		wait 0.5 seconds
		execute console command "/execute %player-arg% ~ ~ ~ /particle largeexplode %x coord of event-location% %y coord of event-location + 2.8% %z coord of event-location% 1 0 1 0 10"
		execute console command "mogsound %player-arg% aafutbeam 1 2"
		execute console command "mogsound %player% minecraft:block.portal.travel 1.3 1.5"
		execute console command "/execute %player-arg% ~ ~ ~ /particle flame %x coord of event-location% %y coord of event-location + 3% %z coord of event-location% 0 0 0 0.1 100"
		
