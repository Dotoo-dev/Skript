command /wither2:
	# executable by: console
	trigger:
		set {_t} to random element out of {witherspawnloc::*}
		spawn a rabbit at {_t}
		set the name of last spawned entity to "Usapyon"
		set the maximum health of last spawned entity to 0.1
		heal last spawned entity
		# broadcast "&7[Lv2] &6Usapyon&f: やあ！うさぴょんだよ！"
		# broadcast "&7[Lv2] &6Usapyon&f: 5ふんのあいだにロビーのどこかにいる"
