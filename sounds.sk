command /sound4 <player>:
	executable by: console
	trigger:
		add 1 to {mod_total%uuid of arg-1%}
		add 1 to {mod_legend%uuid of arg-1%}
		message "&7You got &e+50 exp&7 for creating a legendary mod !!" to player-arg
		add 50 to {expnew%uuid of player-arg%}
