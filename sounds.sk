command /sound1 <player>:
	executable by: console
	trigger:
		add 1 to {mod_total%uuid of arg-1%}
		execute console command "mogsound %arg-1% entity.item.break 1.2 1.2"
		wait 5 tick
		execute console command "mogsound %arg-1% entity.item.break 1.2 1.2"
		wait 5 tick
		execute console command "mogsound %arg-1% entity.item.break 1.2 1.2"
		wait 8 tick
		execute console command "mogsound %arg-1% entity.player.levelup 1.2 1.8"

