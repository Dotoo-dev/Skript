every 1 minutes:
	loop all players:
		subtract 1 from {elixirtimer%loop-player%}
		if {elixirtimer%loop-player%} is less than 1:
			if {fortitude%loop-player%} is 1:
				set {fortitude%loop-player%} to 0
				set maximum health of loop-player to 10
				send "&7Elixir of Fortitude effect has been ended." to loop-player
				
			if {reflex%loop-player%} is 1:
				set {reflex%loop-player%} to 0
				send "&7Elixir of Reflex effect has been ended." to loop-player
				
			if {speed%loop-player%} is 1:
				set {speed%loop-player%} to 0
				send "&7Elixir of Speed effect has been ended." to loop-player
				execute console command "speed walk 1 %loop-player%"
				
			if {wisdom%loop-player%} is 1:
				set {wisdom%loop-player%} to 0
				send "&7Elixir of Wisdom effect has been ended." to loop-player
				
			if {shade%loop-player%} is 1:
				set {shade%loop-player%} to 0
				send "&7Elixir of Void effect has been ended." to loop-player
				
			if {bloodthirst%loop-player%} is 1:
				set {bloodthirst%loop-player%} to 0
				send "&7Elixir of Combust effect has been ended." to loop-player
				
			if {critical%loop-player%}  is 1:
				set {critical%loop-player%} to 0
				send "&7Elixir of Critical effect has been ended." to loop-player
				
on gun reload:
	set {gunreloaded%player%} to now
				
every 10 second:
	loop all players:
		if difference between {gunreloaded%loop-player%} and now is less than 7.5 seconds:
			exit 1 section
			
		if {fortitude%loop-player%} is 1:
			set action bar of loop-player to "&aElixir of Furtitude is active!"
			
		if {reflex%loop-player%} is 1:
			set action bar of loop-player to "&aElixir of Reflex is active!"
			
		if {speed%loop-player%} is 1:
			set action bar of loop-player to "&aElixir of Speed is active!"
			
		if {wisdom%loop-player%} is 1:
			set action bar of loop-player to "&aElixir of Wisdom is active!"
			
		if {shade%loop-player%} is 1:
			set action bar of loop-player to "&aElixir of Void is active!"
			
		if {bloodthirst%loop-player%} is 1:
			set action bar of loop-player to "&aElixir of Combust is active!"
			
		if {critical%loop-player%}  is 1:
			set action bar of loop-player to "&aElixir of Critical is active!"
