command /elixpick <player>:
	executable by: console
	trigger:
		if {blue_pickup_ignore%player-arg%} is not set:
			set {white_pickup_ignore%player-arg%} to 0
			set {yellow_pickup_ignore%player-arg%} to 0
			set {orange_pickup_ignore%player-arg%} to 0
			set {red_pickup_ignore%player-arg%} to 0
			set {pink_pickup_ignore%player-arg%} to 0
			set {purple_pickup_ignore%player-arg%} to 0
			set {blue_pickup_ignore%player-arg%} to 0
				
		open chest with 1 rows named "&fStrange Machine Setting" to player-arg
		wait 1 tick
		
		format slot 0 of player-arg with {blankpanel} named "---" to be unstealable
		
		if {white_pickup_ignore%player-arg%} is not 1:
			format slot 1 of player-arg with light gray dye named "&fWhite Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 1"]
		else if {white_pickup_ignore%player-arg%} is 1:
			format slot 1 of player-arg with light gray dye named "&fWhite Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 1"]
			
		if {yellow_pickup_ignore%player-arg%} is not 1:
			format slot 2 of player-arg with yellow dye named "&fYellow Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 2"]
		else if {yellow_pickup_ignore%player-arg%} is 1:
			format slot 2 of player-arg with yellow dye named "&fYellow Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 2"]
			
		if {orange_pickup_ignore%player-arg%} is not 1:
			format slot 3 of player-arg with orange dye named "&fOrange Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 3"]
		else if {orange_pickup_ignore%player-arg%} is 1:
			format slot 3 of player-arg with orange dye named "&fOrange Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 3"]
			
		if {red_pickup_ignore%player-arg%} is not 1:
			format slot 4 of player-arg with red dye named "&fRed Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 4"]
		else if {red_pickup_ignore%player-arg%} is 1:
			format slot 4 of player-arg with red dye named "&fRed Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 4"]
			
		if {pink_pickup_ignore%player-arg%} is not 1:
			format slot 5 of player-arg with pink dye named "&fPink Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 5"]
		else if {pink_pickup_ignore%player-arg%} is 1:
			format slot 5 of player-arg with pink dye named "&fPink Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 5"]
			
		if {purple_pickup_ignore%player-arg%} is not 1:
			format slot 6 of player-arg with purple dye named "&fPurple Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 6"]
		else if {purple_pickup_ignore%player-arg%} is 1:
			format slot 6 of player-arg with purple dye named "&fPurple Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 6"]
			
		if {blue_pickup_ignore%player-arg%} is not 1:
			format slot 7 of player-arg with light blue dye named "&fBlue Herb Toggle" with lore "&7Pick up: &aOn" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 1 7"]
		else if {blue_pickup_ignore%player-arg%} is 1:
			format slot 7 of player-arg with light blue dye named "&fBlue Herb Toggle" with lore "&7Pick up: &cOff" to run [execute player-arg command "/createpotdayosugoidesyoyattanetaechanpickup 2 7"]
		
		format slot 8 of player-arg with {blankpanel} named "---" to be unstealable
		
command /createpotdayosugoidesyoyattanetaechanpickup <number> <number>:
	trigger:
		if arg-2 is 1:
		#white
			if arg-1 is 1:
				set {white_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {white_pickup_ignore%player%} to 0
				send "test0"
		else if arg-2 is 2:
		#yellow
			if arg-1 is 1:
				set {yellow_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {yellow_pickup_ignore%player%} to 0
		else if arg-2 is 3:
		#orange
			if arg-1 is 1:
				set {orange_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {orange_pickup_ignore%player%} to 0
		else if arg-2 is 4:
		#red
			if arg-1 is 1:
				set {red_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {red_pickup_ignore%player%} to 0
		else if arg-2 is 5:
		#pink
			if arg-1 is 1:
				set {pink_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {pink_pickup_ignore%player%} to 0
		else if arg-2 is 6:
		#purp
			if arg-1 is 1:
				set {purple_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {purple_pickup_ignore%player%} to 0
		else if arg-2 is 7:
		#blue
			if arg-1 is 1:
				set {blue_pickup_ignore%player%} to 1
			else if arg-1 is 2:
				set {blue_pickup_ignore%player%} to 0
		
		execute console command "/elixpick %player%"
				
