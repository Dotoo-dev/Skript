on right click holding iron horse armor or gold horse armor or  diamond horse armor or golden pickaxe:
	if slot 40 of player is not air or golden axe or diamond pickaxe:
		send "&7Please change your offhand to offhand item or air."
		stop

	if difference between {elixircd%player%} and now is less than 3 seconds:
		stop
		
	if name of tool of player contains "Elixir":
	
		set {fortitude%player%} to 0
		set {reflex%player%} to 0
		set {speed%player%} to 0
		set {wisdom%player%} to 0
		set {shade%player%} to 0
		set {bloodthirst%player%} to 0
		set {critical%player%} to 0
		# set {unforgiving%player%} to 0
		
		execute console command "speed walk 1 %player%"
		set maximum health of player to 10
		
		if name of tool of player contains "Fortitude":
			set {fortitude%player%} to 1
			send "&7You drunk Elixir of Fortitude !!"
			send "&7Gaining +6 more maximum hearts !!"
			set maximum health of player to 16
			
		if name of tool of player contains "Reflex":
			set {reflex%player%} to 1
			send "&7You drunk Elixir of Reflex !!"
			send "&730%% chance to dodge any damages !!"
			
		if name of tool of player contains "Speed":
			set {speed%player%} to 1
			send "&7You drunk Elixir of Speed !!"
			send "&7Additional movement speeds !!"
			execute console command "speed walk 1.75 %player%"
			
		if name of tool of player contains "Wisdom":
			set {wisdom%player%} to 1
			send "&7You drunk Elixir of Wisdom !!"
			send "&7Finishing opponents gives you 50%% mana !!"
			
		if name of tool of player contains "Void":
			set {shade%player%} to 1
			send "&7You drunk Elixir of Void !!"
			send "&7+1.5 damage to victim at 15%% chance !!"
			send "&7(60%% if the target was glowing)"
			
		if name of tool of player contains "Combust":
			set {bloodthirst%player%} to 1
			send "&7You drunk Elixir of Combust !!"
			send "&7+10 damage for target at 7%% chance !!"
			send "&7(15%% if the target was burning)"
			
		if name of tool of player contains "Critical":
			set {critical%player%} to 1
			send "&7You drunk Elixir of Critical !!"
			send "&7All of your attacks will have 50%% crit chance !!"
			
		set {elixirtimer%player%} to 5
		# execute console command "/effect %player% minecraft:luck 300 1"
		
		if line 3 of lore of tool of player contains "&b18 &fcharge left":
			set line 3 of lore of tool of player to "&b17 &fcharge left"
		else if line 3 of lore of tool of player contains "&b17 &fcharge left":
			set line 3 of lore of tool of player to "&b16 &fcharge left"
		else if line 3 of lore of tool of player contains "&b16 &fcharge left":
			set line 3 of lore of tool of player to "&b15 &fcharge left"
		else if line 3 of lore of tool of player contains "&b15 &fcharge left":
			set line 3 of lore of tool of player to "&b14 &fcharge left"
		else if line 3 of lore of tool of player contains "&b14 &fcharge left":
			set line 3 of lore of tool of player to "&b13 &fcharge left"
		else if line 3 of lore of tool of player contains "&b13 &fcharge left":
			set line 3 of lore of tool of player to "&b12 &fcharge left"
		else if line 3 of lore of tool of player contains "&b12 &fcharge left":
			set line 3 of lore of tool of player to "&b11 &fcharge left"
		else if line 3 of lore of tool of player contains "&b11 &fcharge left":
			set line 3 of lore of tool of player to "&b10 &fcharge left"
		else if line 3 of lore of tool of player contains "&b10 &fcharge left":
			set line 3 of lore of tool of player to "&b9 &fcharge left"
		else if line 3 of lore of tool of player contains "&b9 &fcharge left":
			set line 3 of lore of tool of player to "&b8 &fcharge left"
		else if line 3 of lore of tool of player contains "&b8 &fcharge left":
			set line 3 of lore of tool of player to "&b7 &fcharge left"
		else if line 3 of lore of tool of player contains "&b7 &fcharge left":
			set line 3 of lore of tool of player to "&b6 &fcharge left"
		else if line 3 of lore of tool of player contains "&b6 &fcharge left":
			set line 3 of lore of tool of player to "&b5 &fcharge left"
		else if line 3 of lore of tool of player contains "&b5 &fcharge left":
			set line 3 of lore of tool of player to "&b4 &fcharge left"
		else if line 3 of lore of tool of player contains "&b4 &fcharge left":
			set line 3 of lore of tool of player to "&b3 &fcharge left"
		else if line 3 of lore of tool of player contains "&b3 &fcharge left":
			set line 3 of lore of tool of player to "&b2 &fcharge left"
		else if line 3 of lore of tool of player contains "&b2 &fcharge left":
			set line 3 of lore of tool of player to "&b1 &fcharge left"
		else if line 3 of lore of tool of player contains "&b1 &fcharge left":
			remove held item from player
			
		set {elixircd%player%} to now
			
		execute console command "/mogsound %player% entity.generic.drink 1 1"
		wait 4 tick
		execute console command "/mogsound %player% entity.generic.drink 1 1"
		wait 4 tick
		execute console command "/mogsound %player% entity.generic.drink 1 1"
		wait 4 tick
		execute console command "/execute %player% ~ ~ ~ /particle witchMagic ~ ~ ~ 1 3 1 0.01 50"
		execute console command "mogsound %player% minecraft:entity.experience_orb.pickup 1.3 1.5" 
		wait 2 tick
		execute console command "mogsound %player% minecraft:entity.experience_orb.pickup 1.3 1.2"
		wait 2 tick
		execute console command "mogsound %player% minecraft:entity.experience_orb.pickup 1.3 1.65"

