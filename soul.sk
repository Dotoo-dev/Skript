command /elixset <number>:
	trigger:
		set {elixir_icon%arg-1%} to held item of player
		set name of held item of player to "&fElixir of %arg-1%"
		broadcast "エリクサー%arg-1%セット: %{elixir_icon%arg-1%}% &f(elixir_icon%arg-1%)"
		
command /setsoul:
	trigger:
		player is op
		set {soulitem_knight2} to held item of player
		send "&a setした"
		
