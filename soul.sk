		format slot 6 of player-arg with {blankpanel} named " " with lore " " to be unstealable
		format slot 7 of player-arg with {blankpanel} named " " with lore " " to be unstealable
		format slot 8 of player-arg with {blankpanel} named " " with lore " " to be unstealable
		format slot 9 of player-arg with {blankpanel} named " " with lore " " to be unstealable
		
		#default
		if {soul_active_default%player%} is not set:
			set {soul_active_default%player%} to 1
		if {soul_active_default%player%} is 1:
			format slot 10 of player-arg with {soulitem_default} named "&bDefault Soul" with lore "&fThis is the default state of||&fevery players where it's||&fmost boring one to play with.|| ||&fBase health: &c❤&f x10|| ||&fAbilities:||&a + Very standard style|| &c- No special abilities||&c - Does not have tier upgrades|| ||&fTier unlocks:||&7 Tier 1: None||&7 Tier 2: None||&7 Tier 3: None" to run [make player execute command "/usesoul default"]
		else:
			format slot 10 of player-arg with {soulitem_default} named "&bDefault Soul" to be unstealable
			
		#knight
		if {unlocked_soul_knight%player%} is 1:
			if {soul_active_knight%player%} is 1:
				format slot 11 of player-arg with {soulitem_knight1} named "&7Activate Soul of Leinhard" with lore "&fSoullinks to one of best||&fwarrior in history, makes you||&fvery resilient to keep you up||&fat frontline.|| ||&fBase health: &c❤&f x14|| ||&fAbilities:||&a + Very high health pool||&c - Slow stamina regen||&c - You can only deal 50%% damage at Long range|| ||&fTier unlocks:||&7 Tier 1: Deals +2 damage when your weapon has 50+ ammo||&7 Tier 2: Not implemented yet||&7 Tier 3: Not implemented yet" to run [make player execute command "/usesoul knight"]
			else:
				format slot 11 of player-arg with {soulitem_knight2} named "&fActivate &bSoul of Reinhard" with lore "&fClick to activate this soul" to run [make player execute command "/knight_activatethesoullikeapro"]
		else:
			format slot 11 of player-arg with {soulitem_knight} named "&7Locked soul" with lore "&fClick with 50 Soul Stones to||&funlock this soul." to run [make player execute command "/knight_unlockthesoullikeapro"]
			
		#hunter
		if {unlocked_soul_hunter%player%} is 1:
			if {soul_active_hunter%player%} is 1:
				format slot 12 of player-arg with {soulitem_hunter1} named "&7Locked soul" with lore "せつめい" to run [make player execute command "/usesoul hunter"]
			else:
				format slot 12 of player-arg with {soulitem_hunter2} named "&fActivate &bSoul of Reinhard" with lore "&fClick to activate this soul" to run [make player execute command "/hunter_unlockthesoullikeapro"]
		else:
			format slot 12 of player-arg with {soulitem_hunter} named "&7Locked soul" with lore "&fClick with 50 Soul Stones to||&funlock this soul." to run [make player execute command "/hunter_unlockthesoullikeapro"]
			
		#assassin
		if {unlocked_soul_assa%player%} is 1:
			if {soul_active_assa%player%} is 1:
				format slot 13 of player-arg with {soulitem_assa1} named "&7Locked soul" with lore "せつめい" to run [make player execute command "/usesoul assa"]
			else:
				format slot 13 of player-arg with {soulitem_assa2} named "&fActivate &bSoul of Reinhard" to run [make player execute command "/assa_unlockthesoullikeapro"]
		else:
			format slot 13 of player-arg with {soulitem_assa} named "&7Locked soul" with lore "&fClick with 50 Soul Stones to||&funlock this soul." to run [make player execute command "/assa_unlockthesoullikeapro"]
		
		
		format slot 14 of player-arg with {soulitem_mage} named "&7Not available" with lore "&8Coming soon..." to be unstealable
