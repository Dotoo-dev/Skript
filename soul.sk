command /giveunb <text>:
	trigger:
		player is op
		execute console command "give %name of player% minecraft:diamond_axe 1 %arg-1% {Unbreakable:1,display:{Name:""§6Unbreakable"",Lore:[""§fRight click while in lobby""]},HideFlags:63,ench:[{id:21,lvl:1}]}"
		disenchant held item of player
		
